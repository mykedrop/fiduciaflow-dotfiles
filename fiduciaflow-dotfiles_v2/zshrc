
# Custom Aliases for FiduciaFlow Dev
alias ffdev='cursor ~/Projects/FiduciaFlow'
alias ffpush='git add . && git commit -m "quick update" && git push'
alias pushlog='cd ~/Projects/FiduciaFlow && git add docs/changelog && read -p "Changelog commit message: " msg && git commit -m "$msg" && git push'
alias changelog='open ~/Projects/FiduciaFlow/docs/changelog'
alias cdfid='cd ~/Projects/FiduciaFlow'
alias testsite='cursor ~/Projects/FiduciaFlow-TEST'
alias trmnl='cat ~/Projects/FiduciaFlow/docs/terminal_snippets.txt'
